<h1 class="Welcome">Edit Community</h1>

<div class="communityForm">
	
<form method="POST" action="/community/<%=community._id%>?_method=PUT">
	<div class="form-group">
		<label for="communityTitle">Title</label>
		<input class="form-control"type="text" name="communityTitle" value="<%=community.name%>">
	</div>
	<div class="form-group">
		<label for="zipCode">Zip Code</label>
		<input class="form-control" type="number" name="zipCode" value="<%=community.zip%>">
	</div>
	<div class="form-group">
		<label for="communityDescription">Description </label>
		<textarea class="form-control" name="communityDescription" value="<%=community.description%>"></textarea>
	</div>
	<br>
	<button>Update Community</button>
</form>
</div>


